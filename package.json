{
  "name": "gh-tldr",
  "version": "1.1.1",
  "description": "Generate a TL;DR summary of your GitHub activity",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gh-tldr": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src"
  },
  "keywords": [
    "github",
    "cli",
    "tldr",
    "activity",
    "summary",
    "claude",
    "ai",
    "anthropic",
    "standup",
    "productivity"
  ],
  "author": "yungweng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yungweng/gh-tldr.git"
  },
  "homepage": "https://github.com/yungweng/gh-tldr#readme",
  "bugs": {
    "url": "https://github.com/yungweng/gh-tldr/issues"
  },
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "execa": "^9.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
